{
  "manifest_version": 3,
  "name": "Coursera Auto",
  "version": "1.0",
  "permissions": ["activeTab", "storage", "tabs"],
  "content_scripts": [
    {
      "matches": ["https://www.coursera.org/*"],
      "js": ["content.js"]
    }
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  }
}
